.main{
  min-height: 800px;
  height: 100%;
  background-color: #f5f5f5;
}
.module {
  height: 100%;
  background-color: #fff;
}
.slide_image {
  width: 100%;
}
.block {
  display: flex;
  margin: 30rpx 20rpx 20rpx 20rpx;
}
.block_left {
  background: linear-gradient(to right, #33d9c6, #4bc2d9, #60adea);
}
.block_midle {
  background: linear-gradient(to right, #fea748, #ff8960, #fe7373);
}
.block_right {
  background: linear-gradient(to right, #33bddd, #5e9cec, #8979f9);
}
.block_left,
.block_midle,
.block_right {
  flex: 1;
  height: 100rpx;
  margin-right: 10rpx;
  border-radius: 10rpx;
  color: #fff;
  box-sizing: content-box;
  padding-left: 10rpx;
  padding-top: 10rpx;
  position: relative;
}
navigator {
  width: 100%;
  height: 100%;
  display: block;
}
.block_right {
  margin-right: 0;
}
.text {
  display: flex;
}
button {
  border: 0 !important;
  font-weight: bold;
}

@font-face {
  font-family: "iconfont";
  src: url("//at.alicdn.com/t/font_2187343_sn0uynj4rca.eot?t=1604730350812"); /* IE9 */
  src: url("//at.alicdn.com/t/font_2187343_sn0uynj4rca.eot?t=1604730350812#iefix")
      format("embedded-opentype"),
    /* IE6-IE8 */
      url("data:application/x-font-woff2;charset=utf-8;base64,")
      format("woff2"),
    url("//at.alicdn.com/t/font_2187343_sn0uynj4rca.woff?t=1604730350812")
      format("woff"),
    url("//at.alicdn.com/t/font_2187343_sn0uynj4rca.ttf?t=1604730350812")
      format("truetype"),
    /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
      url("//at.alicdn.com/t/font_2187343_sn0uynj4rca.svg?t=1604730350812#iconfont")
      format("svg"); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 56rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  position: absolute;
  right: 20rpx;
  bottom: 0;
}
.icon-wuliuxiaocheche{
  bottom: -1px;
}
.icon-001diandongche {
  font-size: 80rpx;
  bottom: -10px;
  right: 8px;
}

.icon-001diandongche:before {
  content: "\e829";
}

.icon-wuliuxiaocheche:before {
  content: "\e620";
}

.icon-kuaidiyuan:before {
  content: "\e612";
}
