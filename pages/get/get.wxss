.main{
  list-style: none;
  min-height: 700px;
  height: 100%;
  background-color: #f5f5f5;
}
.song-address{
  border-radius: 20px 20px 0px 0px;
  background-color: #fff;
  position: relative;
  width: 100%;
  top: -20px;
}
.address,.name,.number,.time{
  border: 1px solid #f5f5f5;
  background-color: #f5f5f5;
  width: 75%;
  height: 35px;
  line-height: 35px;
  border-radius: 20px;
}
.name,.number{
  width: 45%;
}
.first,.third,.point{
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 auto;
}
.first{
  padding-top: 15px;
}
.name-num{
  width: 72%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 10px auto;
}
.name{
  margin-left: 15px;
}
.third{
  padding-bottom: 25px;
}

.express{
  margin: 10px auto;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  width: 90%;
}
.ex1{
  font-weight: bold;
  margin: 0 auto;
}
.del{
  color: #f2ae46;
  font-size: 14px;
}
.express-news{
  position: relative;
  top: -5px;
  width: 100%;
  background-color: #fff;
}
.weui-cell{
  background-color: #f5f5f5;
}
.express-point,.Reward-fee{
  border: 1px solid #f5f5f5;
  background-color: #f5f5f5;
  width: 75%;
  height: 35px;
  line-height: 35px;
  border-radius: 20px;
}
.Reward-fee{
  border-radius: 5px;
  width: 90%;
  height: 40px;
  line-height: 40px;
  margin: 10px auto;
}
.weui-cells{
  margin: 10px auto 0;
  border-radius: 5px;
  width: 90%;
}
.Reward_details{
  width: 90%;
  font-size: 12px;
  color: #9a9a9a;
  margin: 0 auto;
}
.Reward_details text{
  display: inline-block;
}
.express_btn{
  margin: 10px auto 13px;
  width: 90%;
  display: flex;
  justify-content: space-between;
}
view .van-button{
  border-radius: var(--button-border-radius,5px);
  height: 35px;
  line-height: 35px;
  width: 110px !important;
}

